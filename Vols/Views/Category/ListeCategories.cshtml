@model List<Category>

    <h2>Category List</h2>
    <div>
    <table class="table table-hover">
        <tr class="table-dark">
                 <th>Name </th>
             </tr>
        @foreach(var obj in Model)
        {
            <tr>
                <td> @obj.Name</td>
            </tr>
        }
    </table>

    <a class="btn btn-primary" asp-controller="Category" asp-action="Create">Add New Category</a>
    </div>